{
  "name": "@nodecfdi/eslint-config",
  "version": "1.0.0",
  "description": "Base Eslint config for NodeCfdi projects",
  "keywords": [
    "eslint",
    "eslint_config",
    "nodecfdi"
  ],
  "private": false,
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsup"
  },
  "devDependencies": {
    "@nodecfdi/tsconfig": "workspace:*",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "@eslint/js": "^8.57.0",
    "@regru/eslint-plugin-prefer-early-return": "^1.0.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-arrow-return-style": "^1.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vitest": "^0.3.26",
    "typescript-eslint": "^7.2.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "typescript": ">=5.4.2"
  },
  "engines": {
    "node": ">=16"
  }
}
